{
  "name": "@tech-challenge/root",
  "private": true,
  "packageManager": "yarn@4.1.1",
  "workspaces": [
    "apps/**",
    "packages/**"
  ],
  "scripts": {
    "frontend:build": "yarn workspace @tech-challenge/frontend build",
    "frontend:start": "yarn workspace @tech-challenge/frontend dev",
    "frontend:preview": "yarn workspace @tech-challenge/frontend preview",
    "storybook:build": "yarn workspace @tech-challenge/frontend build-storybook",
    "storybook:start": "yarn workspace @tech-challenge/frontend start-storybook",
    "components:build": "yarn workspace @tech-challenge/frontend build-storybook",
    "components:start": "yarn workspace @tech-challenge/frontend start-storybook",
    "data:generate": "yarn workspace @tech-challenge/data-prisma generate",
    "data:seed": "yarn workspace @tech-challenge/data-prisma seed",
    "data:migrate": "yarn workspace @tech-challenge/data-prisma migrate"
  },
  "dependencies": {
    "@prisma/client": "^5.18.0",
    "@tech-challenge/ui-components": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^22.1.0",
    "tsx": "^4.17.0"
  }
}
